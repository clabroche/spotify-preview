{
  "name": "spotify-preview",
  "version": "1.1.6",
  "description": "",
  "main": "index.js",
  "author": {
    "name": "clabroche",
    "email": "corentinlabroche@gmail.com"
  },
  "homepage": "https://github.com/clabroche/spotify-preview",
  "repository": "https://github.com/clabroche/spotify-preview",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=true npm run start",
    "pack": "electron-builder --dir",
    "dist": "electron-builder build --linux --x64"
  },
  "build": {
    "artifactName": "spotify-preview.${ext}",
    "appId": "spotify-preview",
    "mac": {
      "category": "music"
    },
    "linux": {
      "protocols": {
        "name": "SpotifyPreview",
        "schemes": [
          "spotifypreview"
        ]
      },
      "target": [
        "AppImage"
      ],
      "desktop": {
        "Icon": "spotify.png"
      },
      "category": "Music"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "axios": "^0.20.0",
    "carlo": "^0.9.46",
    "fs-extra": "^9.0.1",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "electron": "^10.0.0",
    "electron-builder": "^22.8.0",
    "nodemon": "^2.0.4"
  }
}
